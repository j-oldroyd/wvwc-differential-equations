  <appendix>
    <title>Table of Laplace Transforms</title>
    <table xml:id="table-laplace-transforms">
      <title>Table of Common Laplace Transforms</title>
      <tabular xml:id="tabular-laplace-transforms" top="major">
        <row>
          <cell>
            <m>1</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{1}{s}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>t</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{1}{s^{2}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>t^n</m>, <m>n=0,1,2,\ldots</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{n!}{s^{n+1}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>\sin(kt)</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{k}{s^{2}+k^{2}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>\cos(kt)</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{s}{s^{2}+k^{2}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>\sinh(kt)</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{k}{s^{2}-k^{2}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>\cosh(kt)</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{s}{s^{2}-k^{2}}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>e^{at}</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{1}{s-a}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>e^{at}f(t)</m>
          </cell>
          <cell>
            <m>F(s-a)</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>u(t-a)</m>
          </cell>
          <cell>
            <m>\displaystyle\frac{e^{-as}}{s}</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>u(t-a)f(t-a)</m>
          </cell>
          <cell>
            <m>e^{-as}F(s)</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>f^(n)(t)</m>
          </cell>
          <cell>
            <m>s^nF(s) - s^{n-1}f(0) - s^{n-2}f^{\prime}(0) - \cdots - f^{(n-1)}(0)</m>
          </cell>
        </row>
        <row>
          <cell>
            <m>f(t)\ast g(t)</m>
          </cell>
          <cell>
            <m>F(s)G(s)</m>
          </cell>
        </row>
        <row bottom="major">
          <cell>
            <m>\delta(t-a)</m>
          </cell>
          <cell>
            <m>e^{-as}</m>
          </cell>
        </row>
      </tabular>
    </table>
  </appendix>

